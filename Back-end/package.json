{
  "name": "back-end",
  "version": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "dev": "nodemon src/main.ts",
    "build": "tsc",
    "seed": "ts-node prisma/seed/*.ts",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:reset": "prisma migrate reset --force --skip-seed",
    "test": "vitest --ui --project unit",
    "test:e2e": "vitest --ui --project e2e",
    "test:all": "vitest --ui",
    "test:setup": "dotenv -e .env.test -- npx prisma migrate reset --force --skip-seed && prisma migrate dev",
    "test:integration": "yarn test:setup && vitest --run",
    "start": "prisma migrate deploy  && yarn ts-node src/main.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.14.2",
    "@types/pg": "8.11.10",
    "@types/supertest": "6.0.2",
    "@types/uuid": "10.0.0",
    "@vitest/coverage-istanbul": "1.6.0",
    "@vitest/ui": "1.6.0",
    "nodemon": "3.1.0",
    "supertest": "7.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "uuid": "10.0.0",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "@changesets/cli": "2.27.9",
    "@prisma/client": "5.20.0",
    "@vitest/coverage-v8": "^2.1.3",
    "changeset": "0.2.6",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "nodemon": "3.1.0",
    "pg": "8.13.0",
    "prisma": "5.20.0",
    "unplugin-swc": "1.5.1"
  }
}
