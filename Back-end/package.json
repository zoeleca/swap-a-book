{
  "name": "back-end",
  "version": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "dev": "nodemon src/main.ts",
    "build": "tsc",
    "seed": "ts-node prisma/seeds/*.ts",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:reset": "prisma migrate reset --force --skip-seed",
    "test": "vitest --project unit",
    "test:e2e": "NODE_ENV=test vitest  --project e2e",
    "test:all": "vitest",
    "test:setup": "dotenv -e .env.test -- npx prisma migrate reset --force --skip-seed && npx prisma migrate dev",
    "test:integration": "yarn test:setup && vitest --run",
    "start": "prisma migrate dev  && yarn ts-node src/main.ts",
    "dev:docker": "dotenv -e .env.development nodemon src/main.ts",
    "clean:seed": "ts-node scripts/cleanSeed.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/express-jwt": "^7.4.4",
    "@types/node": "20.14.2",
    "@types/pg": "8.11.10",
    "@types/supertest": "6.0.2",
    "@types/uuid": "10.0.0",
    "@vitest/coverage-istanbul": "1.6.0",
    "@vitest/ui": "1.6.0",
    "dotenv-cli": "^8.0.0",
    "nodemon": "3.1.0",
    "supertest": "7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5",
    "uuid": "10.0.0",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "@changesets/cli": "2.27.9",
    "@prisma/client": "5.20.0",
    "@vitest/coverage-v8": "^2.1.3",
    "changeset": "0.2.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-jwt": "^8.5.1",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "express-openid-connect": "^2.17.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "nodemon": "3.1.0",
    "pg": "8.13.0",
    "prisma": "5.20.0",
    "unplugin-swc": "1.5.1",
    "zod": "^3.25.42"
  },
  "prisma": {
    "seed": "ts-node prisma/seeds/seed.ts"
  }
}
